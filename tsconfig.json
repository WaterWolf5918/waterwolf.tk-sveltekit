{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": false,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "dist",
		"paths": {
			"$lib":["./src/lib"],
			"$lib/*":["./src/lib/*"]
		},
		"rootDirs": [
			"..",
			"./types"
		],
    },
}